{
  "name": "memori",
  "version": "0.1.0",
  "description": "Memori : Never Forget",
  "main": "index.ts",
  "type": "module",
  "bin": "./build/server.js",
  "scripts": {
    "test": "node --experimental-vm-modules --experimental-specifier-resolution=node node_modules/jest/bin/jest.js --testTimeout 60000",
    "cli": "node --loader ts-node/esm --experimental-specifier-resolution=node src/cli.ts",
    "check": "tsc",
    "build": "npm run check && babel ./src --out-dir ./build  --extensions '.ts,.js' --copy-files",
    "start": "node --max_old_space_size=8000 --loader ts-node/esm --experimental-specifier-resolution=node --no-warnings src/server/cluster.ts",
    "package": "pkg . --out-path ./dist --targets node16-linux-x64,node16-macos-x64,node16-win-x64",
    "node": "node --loader ts-node/esm --experimental-specifier-resolution=node --no-warnings"
  },
  "author": "",
  "license": "ISC",
  "pkg": {
    "options": [
      "experimental-modules"
    ],
    "assets": [
      "./build/static/**/*"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      "./node_modules",
      "./dist",
      "./build"
    ],
    "transform": {
      "^.+\\.(ts|tsx)?$": "ts-jest",
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "transformIgnorePatterns": [
      "node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "useESM": true,
        "tsconfig": {
          "allowJs": true
        }
      }
    },
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    }
  },
  "dependencies": {
    "@folder/readdir": "^3.1.0",
    "@icebob/node-memwatch": "^2.1.0",
