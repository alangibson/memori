{
  "name": "@memori/server",
  "version": "0.0.11",
  "description": "Memori : Never Forget",
  "type": "module",
  "author": "",
  "license": "ISC",
  "private": false,
  "bin": {
    "memori-server": "./bin/pm2-npm.js"
  },
  "scripts": {
    "build": "npm run check && npm run test && npm run compile",
    "check": "npm run check:ts && npm run check:format",
    "check:ts": "tsc --noEmit",
    "check:format": "prettier --check 'src/**/*.ts'",
    "check:lint": "eslint 'src/**/*.ts'",
    "clean": "rimraf ./dist",
    "cli": "node --loader ts-node/esm --experimental-specifier-resolution=node --no-warnings src/cli.ts",
    "compile": "npm run compile:ts && npm run compile:types",
    "compile:ts": "babel ./src --out-dir ./dist  --extensions '.ts,.js' --source-maps --copy-files",
    "compile:types": "tsc --emitDeclarationOnly --skipLibCheck",
    "fix:format": "prettier --write 'src/**/*.ts'",
    "fix:lint": "eslint --fix 'src/**/*.ts'",
    "compile:config": "node-config-ts",
    "prepare": "npm run clean && npm run build",
    "start:watch": "cross-env TF_CPP_MIN_LOG_LEVEL=2 nodemon --watch ./src --exec 'npm run build && npm run start'",
    "start": "node --max_old_space_size=4096 --es-module-specifier-resolution=node --no-warnings ./dist/server/cluster.js",
    "test": "node --experimental-vm-modules --experimental-specifier-resolution=node --no-warnings node_modules/jest/bin/jest.js --testTimeout 60000"
  },
  "files": [
    "dist"
  ],
  "nodemonConfig": {
    "ext": "ts,js,mjs,json"
  },
  "jest": {
    "silent": true,
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      "./node_modules",
      "./dist"
    ],
    "transform": {
      "^.+\\.(ts|tsx)?$": "ts-jest",
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "transformIgnorePatterns": [
      "node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "useESM": true,
        "tsconfig": {
          "allowJs": true
        }
      }
    },
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    }
  },
  "dependencies": {
    "@folder/readdir": "^3.1.0",
    "@icebob/node-memwatch": "^2.1.0",
    "@lokidb/full-text-search": "^2.1.0",
    "@omegasoft/typeconf": "^0.6.3",
    "@tensorflow-models/coco-ssd": "^2.2.2",
    "@tensorflow-models/mobilenet": "^2.1.0",
    "@tensorflow/tfjs": "^3.11.0",
    "@tensorflow/tfjs-node": "^3.11.0",
    "blessed": "^0.1.81",
    "buffer-json": "^2.0.0",
    "cheerio": "*",
    "chrome-remote-interface": "^0.31.0",
    "commander": "^8.2.0",
    "cookie-parser": "^1.4.5",
    "cross-fetch": "^3.1.4",
    "debug": "^4.3.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "fdir": "^5.1.0",
    "ffmpeg-static": "^4.4.0",
    "ffmpeg-stream": "^0.7.0",
    "flexsearch": "^0.7.21",
    "html-metadata": "^1.7.1",
    "image-thumbnail": "^1.0.14",
    "jsdom": "^18.0.0",
    "jsdom-global": "^3.0.2",
    "jsonld": "^4.0.1",
    "jsonld-parser": "^0.6.1",
    "localtunnel": "^2.0.2",
    "mime-types": "^2.1.33",
    "minisearch": "^3.1.0",
    "multer": "^1.4.3",
    "multiformats": "^9.4.9",
    "nano": "^9.0.5",
    "nconf": "^0.11.3",
    "node-config-ts": "^3.1.0",
    "node-fetch": "^3.0.0",
    "node-tesseract-ocr": "^2.2.1",
    "node-warc": "^3.3.1",
    "office-document-properties": "^1.1.0",
    "officeparser": "^2.2.2",
    "open": "^8.3.0",
    "passport": "^0.5.0",
    "passport-cookie": "^1.0.9",
    "passport-http-bearer": "^1.0.1",
    "pdf.js-extract": "^0.1.5",
    "pouchdb": "^7.2.2",
    "pouchdb-find": "^7.2.2",
    "pouchdb-upsert": "^2.2.0",
    "prism-media": "^1.3.2",
    "puppeteer": "^10.4.0",
    "rdir": "^0.0.1",
    "rrdir": "^8.2.2",
    "schema-dts": "^1.0.0",
