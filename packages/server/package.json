    "@types/yargs": "^17.0.4",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "babel-jest": "^27.3.1",
    "babel-preset-es2015": "^6.24.1",
    "caxa": "^2.1.0",
    "chai": "^4.3.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.4.1",
    "eslint-config-airbnb": "^19.0.2",
    "jest": "^27.3.1",
    "ngrok": "^4.2.2",
    "nodemon": "^2.0.15",
