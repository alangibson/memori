    "prettier": "^2.5.1",
    "prettier-plugin-json-formats": "^0.4.0",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "typescript": "^4.4.4",
    "web-ext": "^6.5.0"
  }
}
